{
  "name": "excelcalc",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/server.js",
    "start": "npm run server & npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "fetch:rates": "node scripts/fetch-rates.mjs",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "functions:serve": "supabase functions serve",
    "functions:deploy": "supabase functions deploy send-sms --no-verify-jwt",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "storage:setup": "node scripts/setup-storage-bucket.mjs",
    "mcp:verify": "node .mcp/verify-setup.mjs",
    "mcp:install": "npm install --save-dev @modelcontextprotocol/server-filesystem @modelcontextprotocol/server-git"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.45.0",
    "@vis.gl/react-google-maps": "^1.7.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "linkedom": "^0.16.8",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "twilio": "^5.10.4",
    "undici": "^7.16.0",
    "zod": "^4.1.12",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "@types/googlemaps": "^3.43.3",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "playwright": "^1.48.0",
    "postcss": "^8.5.6",
    "supabase": "^2.54.11",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.0",
    "vite": "^5.0.0"
  }
}
